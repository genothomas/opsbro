version: '2'
services:
  http1:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DEMO-HTTP-debian9-http-node-http-1.txt
    networks:
       - demo1


  http2:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DEMO-HTTP-debian9-http-node-http-2.txt
    networks:
       - demo1


  haproxy:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DEMO-HTTP-debian9-http-node-haproxy.txt
    networks:
       - demo1


  client:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DEMO-HTTP-debian9-http-node-client.txt
    networks:
       - demo1



networks:
  demo1:
      driver: bridge

